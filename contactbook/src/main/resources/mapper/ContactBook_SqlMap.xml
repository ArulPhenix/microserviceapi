<mapper namespace="com.amp.contactbook">
	<select id="getContacts"  resultType="com.amp.contactbook.domain.ContactBook" parameterType="int">
		SELECT * FROM CONTACTBOOK WHERE CONTACTID = #{contactId}
	</select>
</mapper>